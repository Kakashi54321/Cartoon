#include<bits/stdc++.h>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	
	while(t--){
	    int n;
	    cin>>n;
	    
	    int a[n];
	    for(int i=0; i<n; i++){
	        cin>>a[i];
	    }
	    int f=0;
	    int i=0,j=n-1;
	    while(i<=j){
	    if(f==0){
	        cout<<a[j]<<" ";
	        j--;
	        f=1;
	    }
	    else if(f==1){
	        cout<<a[i]<<" ";
	        i++;
	        f=0;
	    }
	    }
	    cout<<endl;
	}
	return 0;
}
